@model CumulativeProject3.Models.Students
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Student/List">Back To Students</a>
<div id="containershow" class="my-3">
    <h2>@Model.studentFname @Model.studentLname</h2>
    <p>Student Number: @Model.studentnumber </p>
    <p>Date Enrolled: @Convert.ToDateTime(Model.enroldate).ToString("dd MMM yyyy")</p>
</div>